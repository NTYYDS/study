#include<stdio.h>

int main()
{
    char a = -128;
    //11111111111111111111111110000000-补码
    //11111111111111111111111110000000-整型提升后补码
    printf("%u", a); //%u打印十进制无符号数  4294967168
    return 0;
}


/*
int main()
{
    char a = 128;// 128二进制10000000-补码 （与-128相同）
    printf("%u", a);//输出结果同上
    return 0;
}
*/
